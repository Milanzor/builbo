<h5 class="card-title">Welcome to {{title}}</h5>
<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Path</th>
        <th scope="col">Active</th>
        <th scope="col"></th>
    </tr>
    </thead>
    <tbody>
    {{#each builders}}

        <tr>
            <td>{{@key}}</td>
            <td>{{this.path}}</td>
            <td>
                {{#if this.active}}
                    Active
                {{else}}
                    Inactive
                {{/if}}
            </td>
            <td>
                <a href="/builder/{{@key}}" class="btn btn-primary btn-sm p-1">Open</a>
            </td>
        </tr>
    {{/each}}


    </tbody>
</table>
