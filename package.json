{
  "name": "builly",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/www",
    "start-dev": "node ./bin/www",
    "example": "cross-env yarn start-dev --config ./docs/demo-dirs/example.builders.json",
    "nodemon": "nodemon -i builders.json -i ./docs/demo-dirs/example.builders.json --exec yarn example",
    "travis": "yarn example --travistest",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "ansi-to-html": "^0.6.4",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "helmet": "^3.12.1",
    "http-errors": "~1.6.2",
    "linerstream": "^0.2.0",
    "minimist": "^1.2.0",
    "morgan": "~1.9.0",
    "socket.io": "^2.1.1",
    "tree-kill": "^1.2.0",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "daemonite-material": "^4.1.1",
    "daemonite-material-additions": "^1.0.2",
    "material-design-icons-iconfont-only": "^3.0.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "vue-socket.io": "^2.1.1-b",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
